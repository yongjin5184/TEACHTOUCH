<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC '-//mybatis.org//DTD Config 3.0//EN'
  'http://mybatis.org/dtd/mybatis-3-config.dtd'>

<configuration>
	<settings>
		<setting name='cacheEnabled' value='false' />
		<setting name='useGeneratedKeys' value='true' />
		<setting name='defaultExecutorType' value='REUSE' />
	</settings>
<!--vo 설정  -->
 	<typeAliases>
		<typeAlias type='kr.co.edu.hansung.vo.MemberVO' alias='MemberVO'></typeAlias>
		<typeAlias type='kr.co.edu.hansung.vo.JoinGroupVO' alias='JoinGroupVO'></typeAlias>
		<typeAlias type='kr.co.edu.hansung.vo.GroupVO' alias='GroupVO'></typeAlias>
		<typeAlias type='kr.co.edu.hansung.vo.RequestVO' alias='RequestVO'></typeAlias>
		<typeAlias type='kr.co.edu.hansung.vo.ResponseVO' alias='ResponseVO'></typeAlias>
		<typeAlias type='kr.co.edu.hansung.vo.QnAVO' alias='QnAVO'></typeAlias>
		<typeAlias type='kr.co.edu.hansung.vo.SearchRequestVO' alias='SearchRequestVO'></typeAlias>
		<typeAlias type='kr.co.edu.hansung.vo.NoteVO' alias='NoteVO'></typeAlias>
		<typeAlias type='kr.co.edu.hansung.vo.JoinVOAndGroupVO' alias='JoinVOAndGroupVO'></typeAlias>
	</typeAliases> 
<!--mapper설정  -->
 	 <mappers>
		<mapper resource='sql/mybatis/mapper/member.xml'></mapper>
		<mapper resource='sql/mybatis/mapper/join_group.xml'></mapper>
		<mapper resource='sql/mybatis/mapper/group.xml'></mapper>
		<mapper resource='sql/mybatis/mapper/request.xml'></mapper>
		<mapper resource='sql/mybatis/mapper/response.xml'></mapper>
		<mapper resource='sql/mybatis/mapper/note.xml'></mapper>
	</mappers>  
</configuration>